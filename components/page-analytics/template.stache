			    						<td class="deleteCheckBox">
			    							<img src="resources/images/button_minus_red.png" class="addSocContactsRemove deleteCheckBoxNCD" val="{{name}}"></img>
			    							
			    						</td>
			    						<td class="contactName">
			    							<input type="text" id="name" value="{{contactName}}" can-value="{{canvalueName}}" class="form-control"/>
			    						</td>
			    						<td class="contactEmail">
			    							<input type="text" id="email" value="{{contactEmail}}" can-value="{{canvalueEmail}}" class="form-control"/>
			    						</td>
			    					</tr>
			    				 {{/each}}
			    				</tbody>
			    			</table>
				    		
		    			</div>
		    			
			    	</div>

