				    							<div  style="height:1.5em"><input type="checkbox" value="{{actualName}}"  class="reportBox"  val="false"/> {{name}}</div>
				    						{{/each}}
				    					</td>
				    					

				    				</tr>
			    				 
			    				</tbody>

		    				</table>

		    			</div>
