		    				<input type="text" name="sapVendor" id="sapVendor" can-value="licDetails.data.sapVendorNo" value="{{licDetails.data.sapVendorNo}}" class="form-control sapVendor"/>
						</div>

	    			</div>

					<div class="col-sm-12" id="spaceBar5px"></div>

	    			<div class="col-sm-11 col-sm-offset-1 form-group paymentTerms">

